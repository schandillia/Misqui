    "next": "15.3.1",
    "next-auth": "5.0.0-beta.26",
    "next-themes": "^0.4.6",
    "postgres": "^3.4.5",
    "react": "^19.0.0",
    "react-circular-progressbar": "^2.2.0",
    "react-confetti": "^6.4.0",
