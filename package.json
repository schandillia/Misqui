{
  "name": "misqui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "check": "pnpm lint && pnpm type-check",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:erase": "tsx scripts/erase.cts",
    "db:seed": "tsx scripts/seed.cts",
    "db:reset": "clear && pnpm run db:erase && pnpm run db:generate && pnpm run db:push && pnpm run db:seed",
    "generate:env": "tsx scripts/generate-env-example.mjs",
    "clean": "rm -rf .next && rm -rf node_modules && rm -rf dist && rm -rf coverage",
    "type-check": "tsc --noEmit --pretty",
    "test": "clear && pnpm exec playwright test && pnpm exec playwright show-report",
    "test:auth": "clear && pnpm exec playwright test tests/auth.setup.ts --headed --project=setup --trace=on",
    "test:ui": "clear && pnpm exec playwright test --ui"
  },
  "dependencies": {
